<?xml version="1.0" encoding="utf-8"?>
<merge>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/card_list_container"

        android:elevation="1dp"
        android:layout_margin="10dp"
        android:padding="10dp"
        xmlns:app="http://schemas.android.com/apk/res-auto">
<!--        android:background="@drawable/corner_radius_material_surface"-->
        <LinearLayout
            android:id="@+id/card"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <com.projekt.tdp028.FeedList.CardInfoViewGroup
                android:id="@+id/card_info_group"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/corner_radius_material_primary" />


            <TextView
                android:id="@+id/card_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:textColor="@color/colorDarkMaterialOnSurface"
                android:textSize="15sp" />


        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

</merge>
